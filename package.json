{
  "name": "@anonacy/puppet",
  "type": "module",
  "version": "0.1.2",
  "packageManager": "npm@8.6.12",
  "description": "",
  "author": "hewham",
  "license": "MIT",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/app.d.ts",
      "import": "./dist/app.mjs",
      "require": "./dist/app.cjs"
    }
  },
  "main": "./dist/app.mjs",
  "module": "./dist/app.mjs",
  "types": "./dist/app.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/app.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20",
    "npm": ">=8"
  },
  "scripts": {
    "start": "npx cowsay -f elephant2 Starting Puppet! && npx nodemon --watch 'src/**/*.ts' --exec 'npx tsx' src/app.ts",
    "build": "npx unbuild",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "heroku-postbuild": "npm run build && mkdir ./.cache && mv /app/.cache/puppeteer ./.cache",
    "postinstall": "npx puppeteer browsers install chrome"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.6.4",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "bumpp": "^9.3.0",
    "cowsay": "^1.6.0",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "lint-staged": "^15.2.2",
    "nodemon": "^3.1.0",
    "pnpm": "^8.15.3",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.9.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "unbuild": "^2.0.0",
    "vite": "^5.1.3",
    "vitest": "^1.3.1"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "puppeteer": "22.3.0"
  }
}
